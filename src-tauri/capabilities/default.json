{
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:path:allow-resolve",
    "dialog:allow-open",
    "dialog:allow-confirm",
    "sql:default",
    "sql:allow-execute"
  ],
  "path": {
    "all": true
  },
  "fs": {
    "all": true,
    "scope": [
      "**"
    ]
  }
}